gph = int(input('Digite o ganho por hora: '))
#ganho por hora (gph)
ht = int(input('Digite as horas trabalhadas no mês: '))
#horas trabalhadas (ht)

print('')

bruto = (gph*ht) 
IR = (bruto*11)/100
INSS = (bruto*8)/100
Sindicato = (bruto*5)/100
líquido = bruto - (IR + INSS + Sindicato)

print('Salário Bruto: R$', bruto)
print('Imposto de Renda(11%): R$', IR)
print('INSS(8%): R$', INSS)
print('Sindicato(5%): R$', Sindicato)
print('Salário Líquido: R$', líquido)


